<%= simple_form_for [@report] do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :location %>
  <%= f.input :category, collection: ['Improvement', 'Repair']  %>
  <%= f.input :status, collection: ['Unclaimed', 'In Progress', 'Done'], default:["unclaimed"] %>
  <%= f.input :votes %>
   <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit "Submit report" %>
<% end %>
